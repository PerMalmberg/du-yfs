    "styles": {
        "RouteButtonText": {
            "align": "h0,v1",
            "fill": "r1,g1,b1,a1"
        },
        "RouteButtonTextHover": {
            "align": "h0,v1",
            "fill": "r0,g0.5,b0.5,a1"
        },
        "RouteButtonBox": {
            "fill": "r0,g0,b0,a0",
            "stroke": {
                "color": "r1,g1,b1,a1",
                "distance": 2
            }
        },
        "RouteButtonBoxHover": {
            "fill": "r0,g0,b0,a0",
            "stroke": {
                "color": "r0,g0.5,b0.5,a1",
                "distance": 3
            }
        }
    },
